
reinit

open flxfiles/flx.ctl

set lon 0 360
set lat -90 90
set t 1 480
define pwat=pwatclm
define pwat1=pwat1clm
define pwat2=pwat2clm
define qgcm=spfh2m
define qgcm1=spfh12m
define qgcm2=spfh22m
define lh=lhtflsfc
define sh=shtflsfc
set sdfwrite pwat.nc
sdfwrite pwat
set sdfwrite pwat1.nc
sdfwrite pwat1
set sdfwrite pwat2.nc
sdfwrite pwat2
query file
close 1


